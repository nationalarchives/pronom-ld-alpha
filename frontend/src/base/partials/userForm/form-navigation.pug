      li#contributorsBtn.from-internal-only
        button.segment
          .top-button
            .line &nbsp;
            .round &nbsp;
            span Contributor
            .indicators
              i.fa-solid.fa-triangle-exclamation
              i.fa-solid.fa-square-check
      li#yourDetailsBtn.from-external-only
        button.segment
          .top-button
            .line &nbsp;
            .round &nbsp;
