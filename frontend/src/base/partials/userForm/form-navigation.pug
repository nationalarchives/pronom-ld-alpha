block vars
  - var currentFormSectionw = 1

aside
  .form-nav-container
    .border &nbsp;
    a.sr-only.skip-to-main-content(href="#main-form-content") Skip to Main Content
    ul.main-nav
      li#coreBtn
        button.segment
          .top-button
            .line &nbsp;
            .round &nbsp;
            span Core
            .indicators
              i.fa-solid.fa-triangle-exclamation
              i.fa-solid.fa-square-check
      li#signaturesBtn
        button.segment
          .top-button
            .line &nbsp;
            .round &nbsp;
            span Signature
            .indicators
              i.fa-solid.fa-triangle-exclamation
              i.fa-solid.fa-square-check
      li#priorityBtn.from-external-only
        button.segment.nested
          .top-button
            .line &nbsp;
            .round &nbsp;
