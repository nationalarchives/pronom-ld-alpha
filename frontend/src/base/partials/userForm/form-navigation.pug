            .indicators
              i.fa-solid.fa-triangle-exclamation
              i.fa-solid.fa-square-check
        ul.side-menu
          li#prioritySubBtn
            button.segment-sub Priority
          li#identifiersSubBtn
            button.segment-sub Identifiers
          li#relationshipsSubBtn
