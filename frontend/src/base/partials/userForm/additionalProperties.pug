section.form-part#additionalProperties
  .smallScreen
    button.accordion
      i.fa-solid.fa-chevron-down
      h2 Additional Properties
  .largeScreen
    .from-external-only
      .form-header-main
        h2 More information
        .nav
          button.bordered-blue.medium.left.prevSkip
            i.fa-solid.fa-angles-left
            | Prev
          button.bordered-blue.medium.right.nextSkip
            | Next
            i.fa-solid.fa-angles-right
      .form-header-sub
        h3 Additional Properties
        .nav
          button.bordered-blue.xsmall.left.prev
            i.fa-solid.fa-angles-left
            | Prev
    .from-internal-only
      .form-header-main
        h2 Additional Properties
        .nav
          button.bordered-blue.medium.left.prev
            i.fa-solid.fa-angles-left
            | Prev
          button.bordered-blue.medium.right.next
            | Next
            i.fa-solid.fa-angles-right
  .form-partial-content
    fieldset
      legend.sr-only Provide additional properties - information about byte sequences and developer
      .part
        h3 Byte Sequences
        .part

          //- Byte Order
          .form-row(th:classappend="${compare?.byteOrder} ? 'before-review' : ''")
            p.inputType(th:text="${#strings.isEmpty(ff?.byteOrder) ? 'New content' : 'Edited content'}") New content
            .tooltipPlusAction
              .tooltip-container
                .label
                  label(for="byteOrder") Select file format byte order
                  span.tooltip-toggle(aria-label=htmlWebpackPlugin.options.getTooltip('form', '3.4.1.1') tabindex='0')
                    .i-wrapper
                      i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
              .action-monitoring
                .review-type
                  i.fa-solid.fa-triangle-exclamation(aria-hidden="true")
                  p Require reviewing
                .review-type
                  i.fa-solid.fa-square-check
                  p Reviewed
            .input-group
              button.current Accept current
              select(id="byteOrder", th:field="*{byteOrder}")
                option(th:each="bo : ${byteOrderOptions}", th:value="${bo.value}", th:text="${bo.label}") option 1
            .input-group(th:if="${compare?.byteOrder}")
              button.incoming Accept incoming
              select(id="byteOrder" th:if="${compare?.byteOrder}")
                option(th:each="bo : ${byteOrderOptions}", th:value="${bo.value}", th:text="${bo.label}") option 1

          //- Compression Type
          .form-row(th:classappend="${compare?.compressionType} ? 'before-review' : ''")
            p.inputType(th:text="${#strings.isEmpty(ff?.compressionType) ? 'New content' : 'Edited content'}") New content
            .tooltipPlusAction
              .tooltip-container
                .label
                  label(for="compressionType") Compression Type
                  span.tooltip-toggle(aria-label=htmlWebpackPlugin.options.getTooltip('form', '3.4.1.2') tabindex='0')
                    .i-wrapper
                      i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
              .action-monitoring
                .review-type
                  i.fa-solid.fa-triangle-exclamation(aria-hidden="true")
                  p Require reviewing
                .review-type
                  i.fa-solid.fa-square-check
                  p Reviewed
            .input-group
              button.current Accept current
              select(id="compressionType", th:field="*{compressionType}")
                option(th:each="ct : ${compressionTypeOptions}", th:value="${ct.value}", th:text="${ct.label}") option 1
            .input-group(th:if="${compare?.compressionType}")
              button.incoming Accept incoming
              select(th:if="${compare?.compressionType}")
                option(th:each="ct : ${compressionTypeOptions}", th:value="${ct.value}", th:text="${ct.label}") option 1

        h4.border Stakeholders and specification
        .part
          div
            
            .add-actor
              fieldset(th:each="dev, stat : *{developmentActors}")
                //- Developer
                .form-row(th:classappend="${compare?.developmentActors?.get(__${stat.index}__)?.uri} ? 'before-review' : ''")
                  p.inputType(th:text="${#strings.isEmpty(ff?.developmentActors?.get(__${stat.index}__)?.uri) ? 'New content' : 'Edited content'}") New content
                  .tooltipPlusAction
                    .tooltip-container
                      .label
                        label(th:for="${'developmentActors' + stat.index}") Developer
                        span.tooltip-toggle(aria-label=htmlWebpackPlugin.options.getTooltip('form', '3.4.2.1') tabindex='0')
                          .i-wrapper
                            i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
                    .action-monitoring
                      .review-type
                        i.fa-solid.fa-triangle-exclamation(aria-hidden="true")
                        p Require reviewing
                      .review-type
                        i.fa-solid.fa-square-check
                        p Reviewed
                  .input-group
                    button Accept current
                    input(th:id="${'developmentActors' + stat.index}", th:field="*{developmentActors[__${stat.index}__].uri}", th:data-undo="${ff?.developmentActors?.get(__${stat.index}__)?.uri}")
                  .input-group(th:if="${compare?.developmentActors?.get(__${stat.index}__)?.uri}")
                    button.incoming Accept incoming
                    input(th:id="${'developmentActors' + stat.index}", th:value="${compare.developmentActors?.get(__${stat.index}__)?.uri}")
                p.from-internal-only or
                button.black.small.from-internal-only
                  | Add institutional actor

          div
            .from-internal-only
                .add-actor
                  fieldset(th:each="dev, stat : *{supportActors}")
                    //- Support
                    .form-row(th:classappend="${compare?.supportActors?.get(__${stat.index}__)} ? 'before-review' : ''")
                      p.inputType(th:text="${#strings.isEmpty(ff?.supportActors?.get(__${stat.index}__)?.uri) ? 'New content' : 'Edited content'}") New content
                      .tooltipPlusAction
                        .tooltip-container
                          .label
                            label(th:for="${'supportActors' + stat.index}") Support
                            span.tooltip-toggle(aria-label=htmlWebpackPlugin.options.getTooltip('form', '3.4.2.1') tabindex='0')
                              .i-wrapper
                                i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
                        .action-monitoring
                          .review-type
                            i.fa-solid.fa-triangle-exclamation(aria-hidden="true")
                            p Require reviewing
                          .review-type
                            i.fa-solid.fa-square-check
                            p Reviewed
                      .input-group
                        button Accept current
                        input(th:id="${'supportActors' + stat.index}", th:field="*{supportActors[__${stat.index}__].uri}", th:data-undo="${ff?.supportActors?.get(__${stat.index}__)?.uri}")
                      .input-group(th:if="${compare?.supportActors?.get(__${stat.index}__)?.uri}")
                        button.incoming Accept incoming
                        input(th:id="${'supportActors' + stat.index}", th:value="${compare.supportActors?.get(__${stat.index}__)?.uri}")
                    p.from-internal-only or
                    button.black.small.from-internal-only
                      | Add institutional actor

