section.form-part#additionalProperties
  .smallScreen
    button.accordion
      i.fa-solid.fa-chevron-down
      h2 Additional Properties
  .largeScreen
    .form-header-main
      h2 More information
      .nav
        button.bordered-blue.medium.left
          i.fa-solid.fa-angles-left
          | Prev
        button.bordered-blue.medium.right
          | Next
          i.fa-solid.fa-angles-right
    .form-header-sub
      h3 Additional Properties
      .nav
        button.bordered-blue.xsmall.left
          i.fa-solid.fa-angles-left
          | Prev
        //- button.bordered-blue.xsmall.right
          | Next
          i.fa-solid.fa-angles-right
  .form-partial-content
    fieldset
      legend.sr-only Provide additional properties - information about byte sequences and developer
      .part
        h4 Byte Sequences
        .part
          .form-row
            .tooltip-container
              .label
                label(for="SelectByteOrder") Select file format byte order
                span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                  .i-wrapper
                    i.fa.fa-solid.fa-circle-info(role="img" title="information icon")
            select(id="SelectByteOrder")
              option(value='0') option1
              option(value='1') option2
          .form-row
            .tooltip-container
              .label
                label(for="CompressionType") Compression Type
                span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                  .i-wrapper
                    i.fa.fa-solid.fa-circle-info(role="img" title="information icon")
            input(name="CompressionType" id="CompressionType")
        h4.border Stakeholders and specification
        .part
          .form-row
            .tooltip-container
              .label
                label(for="Developer") Developer
                span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                  .i-wrapper
                    i.fa.fa-solid.fa-circle-info(role="img" title="information icon")
            input(name="Developer" id="Developer")