section.form-part#additionalProperties
  .smallScreen
    button.accordion
      i.fa-solid.fa-chevron-down
      h2 Additional Properties
  .largeScreen
    .from-external-only
      .form-header-main
        h2 More information
        .nav
          button.bordered-blue.medium.left.prevSkip
            i.fa-solid.fa-angles-left
            | Prev
          button.bordered-blue.medium.right.nextSkip
            | Next
            i.fa-solid.fa-angles-right
      .form-header-sub
        h3 Additional Properties
        .nav
          button.bordered-blue.xsmall.left.prev
            i.fa-solid.fa-angles-left
            | Prev
    .from-internal-only
      .form-header-main
        h2 Additional Properties
        .nav
          button.bordered-blue.medium.left.prev
            i.fa-solid.fa-angles-left
            | Prev
          button.bordered-blue.medium.right.next
            | Next
            i.fa-solid.fa-angles-right
  .form-partial-content
    fieldset
      legend.sr-only Provide additional properties - information about byte sequences and developer
      .part
        h4 Byte Sequences
        .part
          .form-row
            .tooltip-container
              .label
                label(for="SelectByteOrder") Select file format byte order
                span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                  .i-wrapper
                    i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
            select(id="SelectByteOrder")
              option(th:each="bo : ${byteOrderOptions}", th:value="${bo.value}", th:text="${bo.label}") option 1
          .form-row
            .tooltip-container
              .label
                label(for="CompressionType") Compression Type
                span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                  .i-wrapper
                    i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
            input(name="CompressionType" id="CompressionType")
        h4.border Stakeholders and specification
        .part
          .form-row
            .from-external-only
              .tooltip-container
                .label
                  label(for="Developer") Developer
                  span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                    .i-wrapper
                      i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
              input(name="Developer" id="Developer")
          .form-row
            .from-internal-only
              .add-actor
                div
                  .tooltip-container
                    .label
                      label(for="Developer") Developer
                      span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                        .i-wrapper
                          i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
                  input(name="Developer" id="Developer")
                p or
                button.black.small
                  | Add institutional actor
          .from-internal-only
              .add-actor
                .form-row
                  .tooltip-container
                    .label
                      label(for="Support") Support
                      span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                        .i-wrapper
                          i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
                  input(name="Support" id="Support")
                p or
                button.black.small
                  | Add institutional actor
              .form-row
                .tooltip-container
                  .label
                    label(for="FormatIPR") Format IPR
                    span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                      .i-wrapper
                        i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
                input(name="FormatIPR" id="FormatIPR")
              .form-row
                .tooltip-container
                  .label
                    label(for="Document") Document
                    span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                      .i-wrapper
                        i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
                input(name="Document" id="Document")
              .form-row
                .tooltip-container
                  .label
                    label(for="ReleaseDate") Release date
                    span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                      .i-wrapper
                        i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
                input.date(name="ReleaseDate" id="ReleaseDate" type="date")
        .from-internal-only
          h4.border Technical enviroment 
          .part
            .form-row
              div
                .tooltip-container
                  .label
                    label(for="technicalEnviroment") Technical enviroment 
                    span.tooltip-toggle(aria-label='Some explanation comes here' tabindex='0')
                      .i-wrapper
                        i.fa.fa-solid.fa-circle-info(role="img" aria-label="information icon")
                input(name="technicalEnviroment" id="technicalEnviroment")