<?xml version="1.0" encoding="UTF-8"?>
<FFSignatureFile th:DateCreated="${#temporals.formatISO(#temporals.createNowForTimeZone('Europe/London'))}" Version="104" xmlns="http://www.nationalarchives.gov.uk/pronom/SignatureFile" xmlns:th="http://www.thymeleaf.org">
    <InternalSignatureCollection><th:block th:each="sig : ${signatures}">
        <InternalSignature th:ID="${sig.ID}" th:Specificity="${sig.specificity}">
            <ByteSequence th:each="bs : ${sig.byteSequences}" th:attrappend="Endianness=${bs.byteOrderName != null}?${bs.byteOrderName}" th:Reference="${bs.reference}"><th:block th:if="${dev}">
            <DevFullSequence th:text="${bs.sequence}"/></th:block>
                <SubSequence th:each="ss : ${bs.subSequences}" th:MinFragLength="${ss.minFragLength}" th:Position="${ss.position}" th:SubSeqMaxOffset="${ss.subSeqMaxOffset}" th:SubSeqMinOffset="${ss.subSeqMinOffset}">
                    <Sequence th:text="${ss.sequence}">49492A00</Sequence><th:block th:each="fr : ${ss.fragments}"><th:block th:switch="${fr.type.name()}">
                    <LeftFragment th:case="'LEFT'" th:MinOffset="${fr.minOffset}" th:MaxOffset="${fr.maxOffset}" th:Position="${fr.position}" th:text="${fr.value}" >FRAG</LeftFragment><RightFragment th:case="'RIGHT'" th:MinOffset="${fr.minOffset}" th:MaxOffset="${fr.maxOffset}" th:Position="${fr.position}" th:text="${fr.value}" >FRAG</RightFragment></th:block></th:block><!--/*<DefaultShift>5</DefaultShift>--><!--<Shift Byte="00">1</Shift>*/-->
                </SubSequence>
            </ByteSequence>
        </InternalSignature></th:block>
    </InternalSignatureCollection>
    <FileFormatCollection>
        <FileFormat th:each="f : ${formats}" th:ID="${f.ID}" th:MIMEType="${f.MIMETypeList}" th:Name="${f.name}" th:PUID="${f.formattedPuid}" th:Version="${f.version}"><th:block th:each="sig : ${f.internalSignatures}">
            <InternalSignatureID th:text="${sig.ID}">415</InternalSignatureID></th:block><th:block th:each="ext : ${f.externalSignatures}">
            <Extension th:text="${ext.name}">mcw</Extension></th:block><th:block th:each="prRel : ${f.hasPriorityOver}">
            <HasPriorityOverFileFormatID th:text="${prRel.targetID}">221</HasPriorityOverFileFormatID></th:block>
        </FileFormat>
    </FileFormatCollection>
</FFSignatureFile>