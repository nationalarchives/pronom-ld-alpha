@prefix rr: <http://www.w3.org/ns/r2rml#>. 
@prefix pr: <http://www.nationalarchives.gov.uk/PRONOM/>
@prefix formatDeveloper: <http://www.nationalarchives.gov.uk/PRONOM/formatDeveloper.>


<#PRONOM6/format_developers>
 	rr:logicalTable [ rr:tableName "PRONOM6.dbo.format_developers" ];
 	rr:subjectMap [
 		rr:template "http://www.nationalarchives.gov.uk/PRONOM/id/FormatDeveloper/{file_format_id}";
 		rr:class pr:FormatDeveloper;
 	];
 	rr:predicateObjectMap [
        rr:predicate formatDeveloper:DeveloperId;
        rr:objectMap [ rr:column "developer_id" ];
    ];
 	.

 <#PRONOM6/format_developers#view> rr:sqlQuery """
SELECT [file_format_id]
     ,[developer_id]
 FROM [PRONOM6].[dbo].[format_developers]
""" .

