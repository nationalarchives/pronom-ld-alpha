@prefix rr: <http://www.w3.org/ns/r2rml#>. 
@prefix pr: <http://www.nationalarchives.gov.uk/PRONOM/>
@prefix softwareIdentifierType: <http://www.nationalarchives.gov.uk/PRONOM/softwareIdentifierType.>


<#PRONOM6/software_identifier_types>
 	rr:logicalTable [ rr:tableName "PRONOM6.dbo.software_identifier_types" ];
 	rr:subjectMap [
 		rr:template "http://www.nationalarchives.gov.uk/PRONOM/id/SoftwareIdentifierType/{identifier_type_id}";
 		rr:class pr:SoftwareIdentifierType;
 	];
 	rr:predicateObjectMap [
        rr:predicate softwareIdentifierType:IdentifierType;
        rr:objectMap [ rr:column "identifier_type_text" ];
    ];
 	.

 <#PRONOM6/software_identifier_types#view> rr:sqlQuery """
SELECT [identifier_type_id]
     ,[identifier_type_text]
 FROM [PRONOM6].[dbo].[software_identifier_types]
""" .

