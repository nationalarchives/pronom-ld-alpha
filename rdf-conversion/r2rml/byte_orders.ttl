@prefix rr: <http://www.w3.org/ns/r2rml#>. 
@prefix pr: <http://www.nationalarchives.gov.uk/PRONOM/>
@prefix byteOrder: <http://www.nationalarchives.gov.uk/PRONOM/byteOrder.>


<#PRONOM6/byte_orders>
 	rr:logicalTable [ rr:tableName "PRONOM6.dbo.byte_orders" ];
 	rr:subjectMap [
 		rr:template "http://www.nationalarchives.gov.uk/PRONOM/id/ByteOrder/{byte_order_id}";
 		rr:class pr:ByteOrder;
 	];
 	rr:predicateObjectMap [
        rr:predicate byteOrder:ByteOrder;
        rr:objectMap [ rr:column "byte_order_text" ];
    ];
 	.

 <#PRONOM6/actors#view> rr:sqlQuery """
SELECT [byte_order_id]
     ,[byte_order_text]
 FROM [PRONOM6].[dbo].[byte_orders]
""" .

