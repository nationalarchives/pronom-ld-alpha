@prefix rr: <http://www.w3.org/ns/r2rml#>. 
@prefix pr: <http://www.nationalarchives.gov.uk/PRONOM/>
@prefix documentAuthor: <http://www.nationalarchives.gov.uk/PRONOM/documentAuthor.>


<#PRONOM6/document_authors>
 	rr:logicalTable <#PRONOM6/document_authors#view>;
 	rr:subjectMap [
 		rr:template "http://www.nationalarchives.gov.uk/PRONOM/id/DocumentAuthor/{document_id}";
 		rr:class pr:DocumentAuthor;
 	];
 	rr:predicateObjectMap [
        rr:predicate documentAuthor:AuthorId;
        rr:objectMap [ rr:column "author_id" ];
    ];
 	.

 <#PRONOM6/document_authors#view> rr:sqlQuery """
SELECT [document_id]
     ,[author_id]
 FROM [PRONOM6].[dbo].[document_authors]
""" .

