@prefix rr: <http://www.w3.org/ns/r2rml#>. 
@prefix pr: <http://www.nationalarchives.gov.uk/PRONOM/>
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

<#PRONOM6/availability>
 	rr:logicalTable <#PRONOM6/availability#view>;
 	rr:subjectMap [
 		rr:template "http://www.nationalarchives.gov.uk/PRONOM/id/Availability/{availability_id}";
 		rr:class pr:Availability;
 	];
 	rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rr:column "availability_text" ];
    ];
 	.

 <#PRONOM6/availability#view> rr:sqlQuery """
SELECT [availability_id]
     ,[availability_text]
 FROM [PRONOM6].[dbo].[availability]
""" .

