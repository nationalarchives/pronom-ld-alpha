@prefix rr: <http://www.w3.org/ns/r2rml#>. 
@prefix pr: <http://www.nationalarchives.gov.uk/PRONOM/>
@prefix documentIdentifier: <http://www.nationalarchives.gov.uk/PRONOM/documentIdentifier.>


<#PRONOM6/document_identifiers>
 	rr:logicalTable [ rr:tableName "PRONOM6.dbo.document_identifiers" ];
 	rr:subjectMap [
 		rr:template "http://www.nationalarchives.gov.uk/PRONOM/id/DocumentIdentifier/{identifier_id}";
 		rr:class pr:DocumentIdentifier;
 	];
 	rr:predicateObjectMap [
        rr:predicate documentIdentifier:IdentifierTypeId;
        rr:objectMap [ rr:column "identifier_type_id" ];
    ];
 	rr:predicateObjectMap [
        rr:predicate documentIdentifier:Identifier;
        rr:objectMap [ rr:column "identifier_text" ];
 	];
 	rr:predicateObjectMap [
        rr:predicate documentIdentifier:DocumentId;
        rr:objectMap [ rr:column "document_id" ];
    ];
 	rr:predicateObjectMap [
        rr:predicate documentIdentifier:Note;
        rr:objectMap [ rr:column "note_text" ];
 	];
 	rr:predicateObjectMap [
        rr:predicate documentIdentifier:SourceId;
        rr:objectMap [ rr:column "source_id" ];
    ];
 	rr:predicateObjectMap [
        rr:predicate documentIdentifier:SourceDate;
        rr:objectMap [ rr:column "source_date" ];
 	];
 	rr:predicateObjectMap [
        rr:predicate documentIdentifier:LastUpdatedDate;
        rr:objectMap [ rr:column "last_updated_date" ];
    ];
 	rr:predicateObjectMap [
        rr:predicate documentIdentifier:Provenance;
        rr:objectMap [ rr:column "provenance_text" ];
 	];
 	.

 <#PRONOM6/document_identifiers#view> rr:sqlQuery """
SELECT [identifier_id]
     ,[identifier_type_id]
     ,[identifier_text]
     ,[document_id]
     ,[note_text]
     ,[source_id]
     ,[source_date]
     ,[last_updated_date]
     ,[provenance_text]
 FROM [PRONOM6].[dbo].[document_identifiers]
""" .

