@prefix rr: <http://www.w3.org/ns/r2rml#>. 
@prefix pr: <http://www.nationalarchives.gov.uk/PRONOM/>
@prefix formatSupport: <http://www.nationalarchives.gov.uk/PRONOM/formatSupport.>


<#PRONOM6/format_support>
 	rr:logicalTable [ rr:tableName "PRONOM6.dbo.format_support" ];
 	rr:subjectMap [
 		rr:template "http://www.nationalarchives.gov.uk/PRONOM/id/FormatSupport/{file_format_id}";
 		rr:class pr:FormatSupport;
 	];
 	rr:predicateObjectMap [
        rr:predicate formatSupport:SupportId;
        rr:objectMap [ rr:column "support_id" ];
    ];
 	.

 <#PRONOM6/format_support#view> rr:sqlQuery """
SELECT [file_format_id]
     ,[support_id]
 FROM [PRONOM6].[dbo].[format_support]
""" .

