@prefix rr: <http://www.w3.org/ns/r2rml#>. 
@prefix pr: <http://www.nationalarchives.gov.uk/PRONOM/>
@prefix formatDocumentation: <http://www.nationalarchives.gov.uk/PRONOM/formatDocumentation.>


<#PRONOM6/format_documentation>
 	rr:logicalTable [ rr:tableName "PRONOM6.dbo.format_documentation" ];
 	rr:subjectMap [
 		rr:template "http://www.nationalarchives.gov.uk/PRONOM/id/FormatDocumentation/{file_format_id}";
 		rr:class pr:FormatDocumentation;
 	];
 	rr:predicateObjectMap [
        rr:predicate formatDocumentation:DocumentId;
        rr:objectMap [ rr:column "document_id" ];
    ];
 	.

 <#PRONOM6/format_documentation#view> rr:sqlQuery """
SELECT [file_format_id]
     ,[document_id]
 FROM [PRONOM6].[dbo].[format_documentation]
""" .

